{
  "name": "@appbricks/identity",
  "version": "0.0.7",
  "description": "Authentication and Identity SDK",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --quiet --fix",
    "build": "tsc",
    "test": "jest",
    "publish": "tsc && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appbricks/node-identity.git"
  },
  "keywords": [
    "AppBricks",
    "authentication",
    "identity"
  ],
  "author": "Mevan Samaratunga",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/appbricks/node-identity/issues"
  },
  "homepage": "https://github.com/appbricks/node-identity",
  "dependencies": {
    "@appbricks/utils": "^0.0.11",
    "aws-amplify": "^3.3.27",
    "axios": "^0.21.1",
    "gravatar": "^1.8.1",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@appbricks/test-utils": "^0.0.11",
    "@types/gravatar": "^1.8.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gmail-tester": "^1.3.2",
    "jest": "^26.6.3",
    "lodash": ">=4.17.21",
    "node-fetch": "3.0.0-beta.9",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json",
    "lib/"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
